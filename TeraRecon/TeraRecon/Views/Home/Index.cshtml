@{
    ViewBag.Title = "Home Page";
}


<div>
   
</div>

<div style="margin-top:50px;">
    <p style="margin-top:50px;">Search for labels</p>
    @using (Html.BeginForm("Search", "Label"))
    {                
    <input placeholder="Start Typing Here..." id="term" name="term">
    <input type="submit" class="btn btn-info" value="Search" />
    }
    <p style="margin-top:50px;">AutoComplete UI for labels</p>
    <input  placeholder="Start Typing Here..." id="autocomplete">
        
</div>

<div style="margin-top:50px;"  class="hidden" id="result">

    <p id="item"></p>
    <p id="label"></p>

</div>


